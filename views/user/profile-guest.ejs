<%- include ../partials/_navbar.ejs%>
<div class="cover-picture">
</div>
</div>
<img src="<%=owner.image%>" class="image--cover" alt="">
<div class="profile-details-container">
  <h3>@<%= owner.username %></h3>
  <p><%= owner.email%></p>
  <h4><strong>Bio</strong></h4>
  <hr>
  <p class="spot-decription"><%= owner.bio%></p>
  <h4><strong>My Spots</strong></h4>
  <hr>
  <%spotByOwner.forEach((spot) =>{%>
  <a href="/spots/<%=spot._id %>">
    <div class="spots-div-profile" style= "background-image: url(<%=spot.image%>);">
    <div class="filter-black-div">
      
    </div>
    <h3 class="spot-names-profile">
      <%=spot.name %>
      </h3>
    </div>  
  </a>
    
  <%})%>

  
</div>